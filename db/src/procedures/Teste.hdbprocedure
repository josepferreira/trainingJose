PROCEDURE "training.procedures::TESTE"(
 IN P_ID INTEGER
)
LANGUAGE SQLSCRIPT
 SQL SECURITY DEFINER
 --DEFAULT SCHEMA "9827C62410384DA98EEDCD3A835EE87E"
AS
BEGIN
    declare i Integer;

    SELECT COUNT(*) INTO i FROM "TRAINING_TESTE_TESTE" WHERE ID = :P_ID;
    IF(i = 0) THEN
        INSERT INTO "TRAINING_TESTE_TESTE" VALUES(:P_ID);
    END IF;
END